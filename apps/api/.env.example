# --- Database ---
DATABASE_URL=postgres://<user>:<password>@<host>/<db>?sslmode=require&channel_binding=require
PRISMA_DATABASE_URL=postgres://<user>:<password>@<pooler-host>/<db>?sslmode=require&channel_binding=require

# --- App ---
APP_ENV=local
PORT=4000

# --- Auth (examples only; replace in .env) ---
JWT_SECRET=replace-with-32+char-random
JWT_EXPIRES_IN=15m
REFRESH_TOKEN_TTL=30d
BCRYPT_COST=12
TOKEN_HASH_PEPPER=
SESSION_MAX_CONCURRENT=10
